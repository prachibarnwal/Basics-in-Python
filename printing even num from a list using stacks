st = []
def push(st,val):
    st.append(val)

def pop(st):
    if len(st) == 0:
        print("Underflow - Stack is Empty")
    else:
        val = st.pop()
        print(val)

a = list(eval(input("Enter a list of numbers : ")))
for num in a :
    if num % 2 ==  0:
        push(st,num)

while st != []:
    pop(st)
