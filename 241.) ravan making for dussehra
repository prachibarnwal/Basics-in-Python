import turtle as t
t1 = t.Turtle()
s = t.Screen()
s.bgcolor("SKY BLUE")
t1.speed(10)

#CODE FOR FACE
t1.penup()
t1.goto(0,122)
t1.pendown()
t1.begin_fill()
t1.color("khaki")
t1.circle(100)
t1.end_fill()
#CODE FOR BOTH EYES
t1.penup()
t1.goto(-50,230)
t1.pendown()
t1.begin_fill()
t1.color("WHITE")
t1.circle(20)
t1.end_fill()
t1.penup()
t1.goto(-50,235)
t1.pendown()
t1.begin_fill()
t1.color("black")
t1.circle(8)
t1.end_fill()

t1.penup()
t1.goto(50,230)
t1.pendown()
t1.begin_fill()
t1.color("WHITE")
t1.circle(20)
t1.end_fill()
t1.penup()
t1.goto(50,235)
t1.pendown()
t1.begin_fill()
t1.color("black")
t1.circle(8)
t1.end_fill()
#CODE FOR NECK
t1.penup()
t1.goto(0,122)
t1.pendown()
t1.right(90)
t1.begin_fill()
t1.color("#e8beac")
t1.forward(20)
t1.left(90)
t1.forward(7)
t1.left(90)
t1.forward(20)
t1.left(90)
t1.forward(14)
t1.left(90)
t1.forward(20)
t1.left(90)
t1.forward(7)
t1.end_fill()
#CODE FOR  BODY
t1.begin_fill()
t1.color("RED")
t1.left(180)
t1.forward(80)
t1.left(90)
t1.forward(180)
t1.left(90)
t1.forward(160)
t1.left(90)
t1.forward(180)
t1.left(90)
t1.forward(160)
t1.end_fill()
#CODE FOR LEFT HAND
t1.begin_fill()
t1.color("YELLOW")
t1.left(45)
t1.forward(190)
t1.left(85)
t1.forward(25)
t1.left(90)
t1.forward(170)
t1.end_fill()
t1.left(180)
t1.penup()
t1.forward(180)
t1.pendown()
t1.begin_fill()
t1.color("GREEN")
t1.right(90)
t1.forward(25)
t1.right(85)
t1.forward(10)
t1.right(94)
t1.forward(25)
t1.end_fill()
#CODE FOR RIGHT HAND
t1.begin_fill()
t1.color("YELLOW")
t1.penup()
t1.goto(80,102)
t1.pendown()
t1.forward(190)
t1.right(85)
t1.forward(25)
t1.right(90)
t1.forward(170)
t1.right(70)
t1.forward(50)
t1.end_fill()
t1.penup()
t1.right(115)
t1.forward(200)
t1.pendown()
t1.begin_fill()
t1.color("GREEN")
t1.right(95)
t1.forward(30)
t1.right(85)
t1.forward(10)
t1.right(94)
t1.forward(30)
t1.right(90)
t1.forward(10)
t1.end_fill()
#CODE FOR TWO LEGS
t1.penup()
t1.goto(80,-80)
t1.pendown()
t1.begin_fill()
t1.color("GREEN")
t1.right(37)
t1.forward(200)
t1.right(90)
t1.forward(40)
t1.right(78)
t1.forward(180)
t1.left(160)
t1.forward(180)
t1.right(80)
t1.forward(50)
t1.right(90)
t1.forward(210)
t1.right(92)
t1.forward(154)
t1.end_fill()
#CODE FOR  BUTTONS
t1.penup()
y = 90
for i in range(5):
    t1.goto(0,y)
    t1.begin_fill()
    t1.color("BROWN")
    t1.pendown()
    t1.circle(8)
    t1.end_fill()
    y -= 40
#CODE FOR  5  right side  HEADS
t1.penup()
x = 150
for i in range(5):
    t1.goto(x,155)
    t1.pendown
    t1.begin_fill()
    t1.color("LemonChiffon")
    t1.pendown()
    t1.circle(50)
    t1.end_fill()
    t1.penup()
    x += 100
#CODE FOR  4  LEFT SIDE HEADS
t1.penup()
x = -150
for i in range(4):
    t1.goto(x,155)
    t1.pendown()
    t1.begin_fill()
    t1.color("LemonChiffon")
    t1.pendown()
    t1.circle(50)
    t1.end_fill()
    t1.penup()
    x -= 100

#NOSE CODE
t1.goto(4.5, 195)
t1.pendown()
t1.fillcolor("BLACK")
t1.begin_fill()
t1.circle(9)
t1.end_fill()
t1.penup()

#MOUTH CODE
t1.goto(-42, 175)
t1.down()
t1.color("BROWN")
t1.right(90)
t1.circle(40, 180)
t1.up()
# draw tongue
t1.goto(-10, 135)
t1.down()
t1.right(180)
t1.fillcolor('deeppink')
t1.begin_fill()
t1.circle(10, 180)
t1.end_fill()
t1.hideturtle()

#CODE FOR SWORD
t1.penup()
t1.goto(193,-50)
t1.pendown()
t1.color("dimgray")
t1.right(40)
t1.forward(40)
t1.left(70)
t1.forward(30)
t1.right(90)
t1.forward(30)
t1.right(90)
t1.forward(30)
t1.left(90)
t1.forward(150)
t1.right(45)
t1.forward(40)
t1.right(75)
t1.forward(40)
t1.right(55)
t1.forward(150)
t1.left(90)
t1.forward(30)
t1.right(90)
t1.forward(30)
t1.right(90)
t1.forward(30)
t1.left(80)
t1.forward(40)
t1.right(90)
t1.forward(25)
t1.left(90)
t1.forward(40)
t1.right(90)
t1.forward(20)
t1.right(90)
t1.forward(40)
t1.left(90)
t1.forward(23)
t1.penup()
t1.right(90)
t1.forward(50)
t1.pendown()
t1.begin_fill()
t1.color("lightgray")
t1.left(3)
t1.forward(140)
t1.right(45)
t1.forward(25)
t1.right(95)
t1.forward(25)
t1.right(35)
t1.forward(160)
t1.right(90)
t1.forward(50)
t1.right(93)
t1.forward(30)
t1.end_fill()

t1.clone()
